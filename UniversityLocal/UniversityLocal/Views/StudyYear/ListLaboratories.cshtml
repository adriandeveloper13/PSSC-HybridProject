@model IEnumerable<University.Models.StudyYear.Laboratory>

@{
    ViewBag.Title = "ListLaboratories";
}

<h2>DDDLaboratories</h2>


<input type="button" name="create-lab-btn" value="Create Laborator " onclick="ocation.href='@Url.Action("AddLaboratory","StudyYear")'"/>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ContentLink)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ContentLink)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
@section Scripts {
    @Scripts.Render("~/bundles/laboratoryScript")
}